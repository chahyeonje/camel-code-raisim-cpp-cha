<?xml version="1.0" ?>
<robot name="test_planar_elbow_2dof">
  <material name="black">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>
  <material name="Aquamarine">
    <color rgba="0.364 0.733 0.608 1.0"/>
  </material>
  <material name="Aqua">
    <color rgba="0.0 0.733 0.733 1.0"/>
  </material>
  <material name="White">
    <color rgba="0.0 0.0 0.0 0.0"/>
  </material>

  <link name="world"/>
  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="base"/>
  </joint>


  <link name="base">
    <visual>
      <origin rpy="0 0 0" xyz="0 2.5 0.0625"/>
      <geometry>
        <box size="0.025 5 0.125"/>
      </geometry>
      <material name="White"/>
    </visual>

    <visual>
      <origin rpy="0 0 0" xyz="2.5 0 0.0625"/>
      <geometry>
        <box size="5 0.025 0.125"/>
      </geometry>
      <material name="White"/>
    </visual>

    <inertial>
      <mass value="10"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="origin" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.125"/>
    <parent link="base"/>
    <child link="origin_cylinder"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="origin_cylinder">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.00625"/>
      <geometry>
        <cylinder length="0.0125" radius="0.0125"/>
      </geometry>
      <material name="Aqua"/>
    </visual>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="0.00203125" ixy="0.0" ixz="0.0" iyy="0.00375000" iyz="0.0" izz="0.00203125"/>
    </inertial>
  </link>

  <joint name="joint1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
    <parent link="origin_cylinder"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="link1">
    <visual>
      <origin rpy="0 0 0" xyz="0.0746 0 0.00625"/>
      <geometry>
        <box size="0.1492 0.01 0.0125"/>
      </geometry>
      <material name="Aqua"/>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0.0746 0 0.00625"/>
      <geometry>
        <box size="0.1492 0.01 0.0125"/>
      </geometry>
    </collision>

  </link>

  <joint name="fixedJoint_link1" type="fixed">
    <origin rpy="0 0 0" xyz="0.1492 0 0"/>
    <parent link="link1"/>
    <child link="point_mass_of_link1"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="point_mass_of_link1">
    <visual>
      <origin rpy="0 0 0" xyz="-0.0001 0 0"/>
      <geometry>
        <box size="0.0001 0.0001 0.0001"/>
      </geometry>
      <material name="white"/>
    </visual>

    <inertial>
      <mass value="0.193"/>
      <!-- <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/> -->
    </inertial>
  </link>

  <joint name="joint2" type="revolute">
    <origin rpy="0 0 0" xyz="0.1492 0 0"/>
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="link2">
    <visual>
      <origin rpy="0 0 0" xyz="0.1905 0 0.00625"/>
      <geometry>
        <box size="0.381 0.01 0.0125"/>
      </geometry>
      <material name="white"/>      
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0.1905 0 0.00625"/>
      <geometry>
          <box size="0.381 0.01 0.0125"/>
      </geometry>
    </collision>
    
  </link>

  <joint name="fixedJoint_link2" type="fixed">
    <origin rpy="0 0 0" xyz="0.381 0 0"/>
    <parent link="link2"/>
    <child link="point_mass_of_link2"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="point_mass_of_link2">
    <visual>
      <origin rpy="0 0 0" xyz="-0.0005 0 0"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
      <material name="white"/>      
    </visual>

    <inertial>
      <mass value="0.073"/>
      <!-- <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/> -->
    </inertial>
  </link>

</robot>